   author_id: ''\n    };\n  },\n  meteor: {\n    $subscribe: {\n      'messages': []\n    },\n    messages: function () {\n      return Messages.find({});\n    }\n  },\n  computed: {\n    user: function () {\n      return this.$store.state.auth.user;\n    }\n  },\n  methods: {\n    fileUpload: function (event) {\n      var func = this;\n      var file = event.currentTarget.files[0];\n      var reader = new FileReader();\n\n      reader.onload = function (event) {\n        var dataUrl = event.target.result;\n        var uploadInstance = Files.insert({\n          file: dataUrl,\n          isBase64: true,\n          fileName: file\n        });\n        uploadInstance.on(\"end\", function (error, fileObj) {\n          if (error) {\n            console.log(error.reason);\n          } else {\n            console.log(fileObj._id);\n          }\n        });\n        uploadInstance.start(); // Meteor.call('fileUpload', file, reader.result, (error) => {\n        //   if (error) {\n        //     alert(error.error)\n        //   } else {\n        //     console.log('file uploaded')\n        //   }\n        // })\n      };\n\n      reader.readAsDataURL(file);\n    },\n    submit: function (event) {\n      var _this = this;\n\n      event.preventDefault();\n      Meteor.call('createMsg', this.text, this.file_id, this.author_id, function (error) {\n        if (error) {\n          alert(error.error);\n        } else {\n          _this.text = '';\n          _this.file_id = '';\n          _this.author_id = '';\n        }\n      });\n    }\n  }\n});","map":{"version":3,"sources":["/home/ramsmunoz/JsProjects/the-chat-room/imports/ui/components/Chat.vue"],"names":["Messages","module","link","v","Files","exportDefault","data","text","file_id","author_id","meteor","$subscribe","messages","find","computed","user","$store","state","auth","methods","fileUpload","event","func","file","currentTarget","files","reader","FileReader","onload","dataUrl","target","result","uploadInstance","insert",