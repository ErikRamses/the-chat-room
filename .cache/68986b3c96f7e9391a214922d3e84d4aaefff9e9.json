urn this.$store.state.auth.user\n    }\n  },\n  methods: {\n    userLogout(event) {\n      event.preventDefault()\n      this.$store.dispatch('logout')\n    },\n    fileUpload(event) {\n      event.preventDefault()\n      let file = event.currentTarget.files[0]\n      let reader = new FileReader()\n      const this_ = this\n      reader.onload = function(event) {\n        let dataUrl = event.target.result\n        this_.file_id = dataUrl\n      }\n      reader.readAsDataURL(file)\n      event.currentTarget.files = []\n    },\n    submit(event) {\n      event.preventDefault()\n      const this_ = this\n      console.log(this_.author_id)\n      Meteor.call('createMsg', this_.text, this_.file_id, this_.author_id, (error) => {\n        if (error) {\n          alert(error.error)\n        } else {\n          this_.text = ''\n          this_.file_id = ''\n        }\n      })\n      let messageDisplay = this.$refs.messageDisplay\n      messageDisplay.scrollTop = messageDisplay.scrollHeight + 140\n    }\n  },\n}\n"]},"sourceType":"module","hash":"68986b3c96f7e9391a214922d3e84d4aaefff9e9"}
